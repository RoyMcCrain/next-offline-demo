{
  "name": "nextjs-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "deploy": "next build && next export",
    "lint": "eslint --fix --quiet 'src/**/*.{ts,tsx}'"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint"
    }
  },
  "importSort": {
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "module"
    }
  }
}
